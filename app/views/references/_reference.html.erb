<% medline = Bio::MEDLINE.new(reference.refblob)
ref = medline.reference
link = "http://www.ncbi.nlm.nih.gov/entrez/query.fcgi?cmd=Retrieve&db=PubMed&dopt=Citation&list_uids="+ref.pubmed-%>
<tr>
  <td class="reference">
    <span class="reference"><%= h ref.format(@user.style) %>
	<%=link_to "Link", link, :popup=>true%></span>
    <br />
    <span class="timestamp">
      Posted <%= time_ago_in_words(reference.created_at) %> ago.
    </span>
  </td>
  <% if current_user?(reference.user) %>
  <td>
    <%= link_to "delete", reference, :method => :delete,
                                     :confirm => "You sure?" %>
  </td>
  <% end %>
</tr>
